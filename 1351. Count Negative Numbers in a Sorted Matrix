class Solution {
public:
    int countNegatives(vector<vector<int>>& grid) {
        int m=grid.size();
        int n=grid[0].size();
        int j=0;
        int sum=0;
        while(j<n){
            for(int i=0;i<m;i++){
                if(grid[i][j]<0){ 
                    sum+=m-i;
                break;}
            }
            j++;
           
        }
    return sum;}
};
    
