class Solution {
public:
    string interpret(string c) {
        string s="";
        for(int i=0;i<c.size();i++){
            if(c[i]=='G')
            s+=c[i];
            else if(c[i]=='(' and c[i+1]==')')
            s+='o';
            else if(c[i]=='(' && c[i+1]=='a')
            s+="al";
        }
        return s;
    }
};
